                            imgOverlay.setAttribute('src', 'https://asset-1.tstatic.net/img/logo/tribun/svg/tribunnews.svg');
                            imgOverlay.setAttribute('alt', 'Tribunnews.com');
                            imgOverlay.setAttribute('loading', 'lazy');
                            imgOverlay.setAttribute('width', 74);
                            imgOverlay.setAttribute('height', 18);
                            imgOverlayWrapper.appendChild(imgOverlay);

                            imgWrapper.appendChild(imgElm);
                            imgWrapper.appendChild(imgOverlayWrapper);

                            var tracker_params = resultObj.engine['tracker_params'];
                            
                            // checking query params in item.url
                            var tempItemUrl = new URL(item.url)
                            if (tempItemUrl.search) {
                                tracker_params = tracker_params.replace('?', '&')
                            }

                            recItem.dataset.url = item.url;
                            var recItemWrapperA = createElement('a', null, null, null);
                            recItemWrapperA.setAttribute('href', item.url + tracker_params);
                            
                            var titleDiv = createElement('div', null, null, [cssClasses.title], item.title);

                            var textWrapper = createElement('div', null, null, [cssClasses.text_wrapper]);
                            textWrapper.appendChild(titleDiv);

                            recItemWrapper.appendChild(imgWrapper);
                            recItemWrapper.appendChild(textWrapper);

                            recItemWrapperA.appendChild(recItemWrapper);

                            recItem.appendChild(recItemWrapperA);

                            widgetWrapper.appendChild(recItem);
                            // hook up the click handler for this item
                            // recItem.onclick = handleClick.bind(null, jxRecHelper, item.url + tracker_params, index); 
                            recItemWrapperA.onclick = handleClick.bind(null, jxRecHelper, item.url + tracker_params, index);
                        });
                    }
                    var endDate = new Date();
                    var loadtime = (endDate.getTime() - startDate.getTime());
                    // trackEventGa('Tribun Jixie Article Recommendation', 'Content Loaded', 'algsrc='+resultObj.engine['tracker_algsrc']+',site=NEWS,device=mobile,loadtime=' + loadtime)
                    console.log(loadtime);
                    resolve();

                  
                    jxRecHelper.items(widgetItemArr);
                  
                    if(resultObj.engine['type'] == 'jixie-api'){
                        jxRecHelper.ready(resultObj.options.algo + ":" + resultObj.options.version);
                    }else{
                        jxRecHelper.ready(resultObj.engine['type']);
                    }

                } else {
                    jxRecHelper.error(204);
                    console.error("Error: no recommendation items");
                    return;
                }
            } catch (err) {
                jxRecHelper.error(901);
                console.log(err.stack);
            }
        });
    }
    var _jxRecSdkURL = 'https://scripts.jixie.media/jxrecsdk.1.0.min.js';
    var startDate = new Date();
    function OneWidget() {
            this._options = {
                accountid: '49294e929f73c07439de2f2801b98a58',
                pageurl: thisPageURL,
                widget_id: '1000013-dbXmIcdj8j',
                type: 'pages',
                container: 'artikel-rekomendasi-jixie',
                keywords: keywordsDL,
                title: titleDL,
                algo: 'contextual',
                customid: '1',
                count: 8
            };
            this._containerId = this._options.container;
            this._container = document.getElementById(this._containerId);
            this._engines = [
                {"url":"https://recommendation.jixie.media/v1/recommendation","method":"GET","pageurl":thisPageURL,"system":"jx","algo":"contextual","type":"jixie-api","tracker_params":"?source=widgetArtikelRekomendasi&engine=JXA","tracker_algsrc":"JXA"}
            ];
        OneWidget.prototype.kickOff = function() {
            startDate = new Date();
            var rand = Math.floor(Math.random() * 1000);
            var randSystem = Math.floor(Math.random() * 1);
            this._engine = this._engines[randSystem]
            this._options.system = this._engine['system']
            // trackEventGa('Tribun Jixie Article Recommendation', 'Widget Load', 'algsrc='+this._engine['tracker_algsrc']+',site=NEWS,device=mobile')
            var promJXSDK = fetchJSFileP(_jxRecSdkURL);
            var thisObj = this;

            var recHelperObj = null;
            var recResults = null;
            promJXSDK
                .then(function() {
                    recHelperObj = jxRecMgr.createJxRecHelper(thisObj._options);

                    var basicInfo = thisObj._options;
                    basicInfo.engine = thisObj._engine

                    var options = {
                        count: basicInfo['count'],
                        adpositions: "",
                        keywords: basicInfo['keywords'],
                        title: basicInfo['title'],
                        date_published: "2023-03-10 09:44:06",
                        pagecategory: "Jayapura",
                        algo: "contextual"
                    };

                    if (basicInfo.engine.type == 'jixie-api') {
                      return recHelperObj.getJxRecommendations(options);
                    } else {
                      return fetchRecommendationsP(basicInfo, recHelperObj.getJxUserInfo());
                    }
                })
                .then(function(resp) {
                    recResults = resp; 
                    recResults.engine = thisObj._engine;
                    if (!resp || !recResults.items || recResults.items.length == 0) {
                        recHelperObj.error(204);
                        throw "no recommendation results";
                    }
                })
                .then(function() {
                    document.getElementById("artikel-rekomendasi-jixie").classList.remove("-loading");
                    document.querySelector('.arjW-list').remove();
                    createDisplay(rand, thisObj._container, recResults, recHelperObj)
                        .then(function() {
                    })
                })
                .catch(function(error) {
                    console.log('Unable to create recommendations widget'+ error.stack + error.message);
                });
        }
    }

    var limit_gtm_ready = 1;

    function check_gtm_ready() {
        if ('ga' in window) {
            // create a new instance of our widget based on the options
            var newW = new OneWidget();
            newW.kickOff();
        } else {
            var timeout_gtm_ready = setTimeout(check_gtm_ready, 500)
            limit_gtm_ready += 1;
            if (limit_gtm_ready > 10) {
                clearTimeout(timeout_gtm_ready);
                var newW = new OneWidget();
                newW.kickOff();
            }
        }
    }
    check_gtm_ready()
</script>
